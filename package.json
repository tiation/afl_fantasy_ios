  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "dev:debug": "NODE_ENV=development DEBUG=1 tsx server/index.ts",
    "preview": "npm run build && npm run start",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:frontend": "vite build",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:node": "node --test --experimental-test-coverage",
    "test:node:watch": "node --test --watch",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix --max-warnings=0",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "quality": "npm run format && npm run lint && npm run check && npm run test",
    "audit:security": "npm audit --audit-level=moderate",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "docker:build": "docker build -t afl-fantasy-platform .",
    "docker:run": "docker run -p 5000:5000 afl-fantasy-platform",
    "health-check": "curl -f http://localhost:5173/api/health || exit 1"
