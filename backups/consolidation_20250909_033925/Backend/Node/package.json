{
  "name": "afl-fantasy-api",
  "version": "1.0.0",
  "description": "AFL Fantasy iOS Backend API - Node.js Express Service",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=20.11",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch index.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint **/*.ts",
    "lint:fix": "eslint **/*.ts --fix",
    "format": "prettier -w .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.7",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "validator": "^13.11.0",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "csv-parser": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/validator": "^13.11.7",
    "@types/multer": "^1.4.11",
    "@types/node-cron": "^3.0.11",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "vitest": "^1.0.4",
    "@vitest/coverage-v8": "^1.0.4",
    "eslint": "^8.56.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.1.1",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "afl",
    "fantasy",
    "api",
    "express",
    "nodejs",
    "typescript",
    "ios"
  ],
  "author": "AFL Fantasy iOS Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/afl-fantasy-ios.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/afl-fantasy-ios/issues"
  },
  "homepage": "https://github.com/your-org/afl-fantasy-ios#readme"
}
